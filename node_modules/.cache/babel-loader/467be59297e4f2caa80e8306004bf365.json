A,QAAA;AAHA,OAAA,EAIA,IAJA,CAIA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA;;AACA,QAAA,MAAA,CAAA,MAAA,CAAA,QAAA,CAAA,YAAA;AACA,OAPA;AAQA,KA3BA;AA4BA,IAAA,UA5BA,sBA4BA,MA5BA,EA4BA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACA,gBAAA,IADA,GACA;AACA,kBAAA,MAAA,EAAA;AADA,iBADA;AAAA;AAAA,uBAIA,MAAA,CAAA,MAAA,CAAA,QAAA,CAAA,aAAA,EAAA,IAAA,CAJA;;AAAA;AAKA,gBAAA,UAAA,CAAA,YAAA;AACA,kBAAA,MAAA,CAAA,QAAA;;AACA,kBAAA,MAAA,CAAA,MAAA,CAAA,QAAA,CAAA,YAAA;AACA,iBAHA,EAGA,GAHA,CAAA;;AALA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,KArCA;AAsCA,IAAA,MAtCA,kBAsCA,OAtCA,EAsCA;AACA,WAAA,OAAA,CAAA,IAAA,kBAAA,OAAA;AACA,KAxCA;AAyCA,IAAA,MAzCA,oBAyCA;AACA,MAAA,YAAA,CAAA,UAAA,CAAA,OAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,GAAA;AACA,KA5CA;AA6CA,IAAA,MA7CA,oBA6CA;AAAA;;AACA,MAAA,IAAA,CAAA,GAAA,CAAA,sBAAA,EAAA;AACA,QAAA,QAAA,EAAA,KAAA,IAAA,CAAA,QADA;AAEA,QAAA,KAAA,EAAA,KAAA,IAAA,CAAA,KAFA;AAGA,QAAA,KAAA,EAAA,KAAA,IAAA,CAAA,KAHA;AAIA,QAAA,OAAA,EAAA,KAAA,IAAA,CAAA;AAJA,OAAA,EAKA,IALA,CAKA,YAAA;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,KAAA;AACA,OAPA;AAQA,KAtDA;AAuDA,IAAA,QAvDA,sBAuDA;AAAA;;AACA,UAAA,KAAA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,MAAA,KAAA,CAAA,EAAA;AACA,eAAA,KAAA,CAAA,gCAAA,CAAA;AACA;;AACA,MAAA,IAAA,CAAA,IAAA,CAAA,gBAAA,EAAA;AACA,QAAA,OAAA,EAAA,KAAA,KAAA,CAAA,KAAA,CAAA,CAAA,EAAA,OADA;AAEA,QAAA,MAAA,EAAA;AAFA,OAAA,EAGA,IAHA,CAGA,YAAA;AACA,QAAA,MAAA,CAAA,QAAA;;AACA,QAAA,MAAA,CAAA,MAAA,CAAA,QAAA,CAAA,YAAA;AACA,OANA;AAOA,KAlEA;AAmEA,IAAA,QAnEA,sBAmEA;AACA,WAAA,IAAA,GAAA,IAAA;AACA;AArEA,GADA;AAyEA,EAAA,IAzEA,kBAyEA;AACA,WAAA;AACA,MAAA,IAAA,EAAA,EADA;AAEA,MAAA,OAAA,EAAA,CAFA;AAGA,MAAA,KAAA,EAAA,EAHA;AAIA,MAAA,IAAA,EAAA,EAJA;AAKA,MAAA,IAAA,EAAA,KALA;AAMA,MAAA,SAAA,EAAA;AANA,KAAA;AAQA,GAlFA;AAmFA,EAAA,OAnFA,qBAmFA;AAAA;;AACA,IAAA,IAAA,CAAA,GAAA,CAAA,YAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,MAAA,MAAA,CAAA,IAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA;AACA,KAFA;AAGA,SAAA,MAAA,CAAA,QAAA,CAAA,YAAA;AACA,SAAA,MAAA,CAAA,QAAA,CAAA,WAAA;AACA;AAzFA,CAAA","sourcesContent":["<template>\n  <v-app>\n    <v-app-bar app dark color=\"rgb(116, 116, 252)\">\n      <v-toolbar-title @click=\"homepage()\" style=\"cursor: pointer\">\n        APP FOOD</v-toolbar-title\n      >\n      <v-spacer />\n      <v-btn text rounded v-b-toggle.sidebar-right\n        ><img\n          class=\"icon\"\n          src=\"https://cdn.pixabay.com/photo/2015/12/23/01/14/edit-1105049_960_720.png\"\n        />\n        <div class=\"number d-flex justify-content-center align-items-start\">\n          <p class=\"num mt-1\">{{ this.$store.state.food.Quanty }}</p>\n        </div></v-btn\n      >\n      <v-btn text rounded v-b-toggle.sidebar-variant\n        ><img\n          class=\"icon\"\n          src=\"https://freeiconshop.com/wp-content/uploads/edd/clipboard-list-flat.png\"\n        />\n        <div class=\"number d-flex justify-content-center align-items-start\">\n          <p class=\"num mt-1\">{{ this.$store.state.food.bill.length }}</p>\n        </div></v-btn\n      >\n      <v-btn text rounded v-b-toggle.sidebar-1\n        ><img\n          class=\"icon user\"\n          src=\"https://www.ebanhang.vn/app/webroot/img/icon_login.png\"\n      /></v-btn>\n    </v-app-bar>\n    <v-content>\n      <router-view />\n    </v-content>\n    <template>\n      <v-footer dark padless>\n        <div v-for=\"item in list\" :key=\"item\">\n          <v-btn text rounded @click=\"showFood(item.cateId)\">{{\n            item.cateName\n          }}</v-btn>\n        </div>\n      </v-footer>\n    </template>\n    <b-sidebar\n      @shown=\"showCart\"\n      id=\"sidebar-right\"\n      title=\"Giỏ Hàng\"\n      bg-variant=\"dark\"\n      text-variant=\"light\"\n      right\n      backdrop\n      width=\"550px\"\n      shadow\n    >\n      <template #footer=\"{}\">\n        <div class=\"d-flex bg-dark text-light align-items-center px-3 py-2\">\n          <strong class=\"mr-auto\">Tổng</strong>\n          <strong class=\"mr-auto\">{{ items ? formatCash(items.total) : 0 }}</strong>\n          <b-button size=\"sm\" variant=\"info\" v-b-modal.modal-1\n            >Thanh Toán</b-button\n          >\n        </div>\n      </template>\n      <div class=\"px-3 py-2\">\n        <b-row>\n          <b-col><b>Tên món</b></b-col>\n          <b-col><b>Hình ảnh</b></b-col>\n          <b-col><b>Số lượng</b></b-col>\n          <b-col><b>Đơn giá</b></b-col>\n          <b-col></b-col>\n        </b-row>\n        <div v-for=\"item in items ? items.items : []\" :key=\"item\">\n          <b-row>\n            <b-col>{{ item.foodName }}</b-col>\n            <b-col\n              ><img class=\"img_cart\" :src=\"item.images[0].imageUrl\" alt=\"\"\n            /></b-col>\n            <b-col\n              ><input\n                type=\"number\"\n                min=\"0\"\n                class=\"input\"\n                v-model=\"item.quantity\"\n                @change=\"editQuanty(item.orderId, item.foodId, item.quantity)\"\n            /></b-col>\n            <b-col>{{ formatCash(item.price) }}</b-col>\n            <b-col\n              ><img\n                @click=\"deleteFood(item.foodId)\"\n                class=\"icon_delete\"\n                src=\"https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTNQ8CLeXOGmVx62VfeFd3eH2elD1EUYLJ0WISuby6szkUl3P6PLW6Djxuy1yOlLkcCMNs&usqp=CAU\"\n            /></b-col>\n          </b-row>\n        </div>\n      </div>\n    </b-sidebar>\n    <b-sidebar\n      id=\"sidebar-1\"\n      title=\"Thông tin cá nhân\"\n      shadow\n      backdrop\n      @shown=\"showInfo\"\n    >\n      <template #footer=\"{}\">\n        <div\n          class=\"d-flex justify-content-between align-items-center px-3 py-2\"\n        >\n          <b-button size=\"sm\" variant=\"warning\" @click=\"editInfo\"\n            >Sửa thông tin</b-button\n          >\n          <b-button size=\"sm\" variant=\"danger\" @click=\"Logout\"\n            >Đăng xuất</b-button\n          >\n        </div>\n      </template>\n      <div class=\"px-3 py-2\">\n        <div v-if=\"!edit\">\n          <p>Họ tên : {{ user.fullName }}</p>\n          <p>Email : {{ user.email }}</p>\n          <p>Số điện thoại : {{ user.phone }}</p>\n          <p>Địa chỉ : {{ user.address }}</p>\n          <p>Phân Quyền : {{ user.role }}</p>\n        </div>\n\n        <div v-if=\"edit\">\n          <div class=\"d-flex justify-content-center\">\n            <p>Họ tên</p>\n            <b-form-input class=\"input\" v-model=\"user.fullName\"></b-form-input>\n          </div>\n          <div class=\"d-flex justify-content-center\">\n            <p>Địa chỉ</p>\n            <b-form-input class=\"input\" v-model=\"user.address\"></b-form-input>\n          </div>\n          <div class=\"d-flex justify-content-center\">\n            <p>Số điện thoại</p>\n            <b-form-input class=\"input\" v-model=\"user.phone\"></b-form-input>\n          </div>\n          <div class=\"d-flex justify-content-center\">\n            <p>Email</p>\n            <b-form-input class=\"input\" v-model=\"user.email\"></b-form-input>\n          </div>\n          <b-button size=\"sm\" variant=\"success\" @click=\"submit\"\n            >Submit</b-button\n          >\n        </div>\n        <b-img\n          src=\"https://picsum.photos/500/500/?image=54\"\n          fluid\n          thumbnail\n        ></b-img>\n      </div>\n    </b-sidebar>\n    <b-modal id=\"modal-1\" title=\"Xác nhận thanh toán\" @ok=\"handleOk\">\n      <p class=\"my-4\">\n        Bạn sẽ thanh toán hóa đơn với số tiền {{ items ? formatCash(items.total) : \"\" }} ?\n      </p>\n    </b-modal>\n    <b-sidebar\n      id=\"sidebar-variant\"\n      title=\"Sidebar\"\n      bg-variant=\"dark\"\n      width=\"450px\"\n      backdrop\n      text-variant=\"light\"\n      shadow\n    >\n      <div class=\"px-3 py-2\">\n        <b-row>\n          <b-col cols=\"3\"><b>ID</b></b-col>\n          <b-col><b>Trạng Thái</b></b-col>\n          <b-col><b>Ngày Đặt</b></b-col>\n        </b-row>\n        <div v-for=\"item in this.$store.state.food.bill\" :key=\"item\">\n          <b-row @click=\"detail(item.orderId)\">\n            <b-col cols=\"3\">{{ item.orderId }}</b-col>\n            <b-col\n              ><b-button\n                :variant=\"item.status === 'PENDING' ? 'warning' : 'success'\"\n                >{{ item.status }}</b-button\n              ></b-col\n            >\n            <b-col>{{ item.updatedAt | moment(\"dddd, MMMM Do YYYY\") }}</b-col>\n          </b-row>\n        </div>\n      </div>\n      <div class=\"px-3 py-2\">\n        <b-table striped hover :items=\"orderList\"></b-table>\n      </div>\n    </b-sidebar>\n  </v-app>\n</template>\n\n<script>\nimport { HTTP } from \"../API/http_common\";\nexport default {\n  methods: {\n    showFood(id) {\n      this.$store.dispatch(\"listFood\", id);\n    },\n    formatCash(value) {\n      let val = (value / 1).toFixed(0).replace(\".\", \",\");\n      return val.toString().replace(/\\B(?=(\\d{3})+(?!\\d))/g, \",\");\n    },\n    async showInfo() {\n      await HTTP.get(\"/user/profile\").then((res) => {\n        this.user = res.data.data;\n      });\n    },\n    showCart() {\n      HTTP.get(\"/order/shopping-cart\").then((res) => {\n        this.items = res.data.data;\n      });\n    },\n    editQuanty(orderId, foodId, quantity) {\n      HTTP.post(\"/order/update\", {\n        orderId,\n        foodId,\n        quantity: parseInt(quantity),\n      }).then(() => {\n        this.showCart();\n        this.$store.dispatch(\"getNumQuan\");\n      });\n    },\n    async deleteFood(foodId) {\n      const data = {\n        foodId: foodId,\n      };\n      await this.$store.dispatch(\"deleteBills\", data);\n      setTimeout(() => {\n        this.showCart();\n        this.$store.dispatch(\"getNumQuan\");\n      }, 100);\n    },\n    detail(orderId) {\n      this.$router.push(`detail/${orderId}`);\n    },\n    Logout() {\n      localStorage.removeItem(\"token\");\n      this.$router.push(\"/\");\n    },\n    submit() {\n      HTTP.put(\"/user/profile/update\", {\n        fullName: this.user.fullName,\n        email: this.user.email,\n        phone: this.user.phone,\n        address: this.user.address,\n      }).then(() => {\n        this.edit = false;\n      });\n    },\n    handleOk() {\n      if (this.$store.state.food.Quanty === 0) {\n        return alert(\"Vui lòng chọn ít nhất 1 món ăn\");\n      }\n      HTTP.post(\"/order/confirm\", {\n        orderId: this.items.items[0].orderId,\n        status: \"CONFIRM\",\n      }).then(() => {\n        this.showCart();\n        this.$store.dispatch(\"getNumQuan\");\n      });\n    },\n    editInfo() {\n      this.edit = true;\n    },\n  },\n\n  data() {\n    return {\n      list: [],\n      numQuan: 0,\n      items: [],\n      user: {},\n      edit: false,\n      orderList: [],\n    };\n  },\n  created() {\n    HTTP.get(\"/cate/list\").then((res) => {\n      this.list = res.data.data;\n    });\n    this.$store.dispatch(\"getNumQuan\");\n    this.$store.dispatch(\"listBills\");\n  },\n};\n</script>\n<style lang='scss' src='../style.scss' >\n</style> \n"],"sourceRoot":"src/components"}]}