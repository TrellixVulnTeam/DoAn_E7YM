A,UAAA,GAAA,EAAA;AACA,kBAAA,KAAA,CAAA,QAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,QAAA;AACA,kBAAA,KAAA,CAAA,KAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,MAAA,CAAA,CAAA,EAAA,QAAA;AACA,kBAAA,KAAA,CAAA,WAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,WAAA;AACA,kBAAA,KAAA,CAAA,KAAA,GAAA,GAAA,CAAA,IAAA,CAAA,IAAA,CAAA,KAAA;AACA,kBAAA,KAAA,CAAA,OAAA,GAAA,OAAA;AACA,iBANA,CADA;;AAAA;AAQA,gBAAA,KAAA,CAAA,KAAA,CAAA,SAAA,EAAA,IAAA;;AARA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AASA,KAVA;AAWA,IAAA,SAXA,qBAWA,MAXA,EAWA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,IAAA,CAAA,IAAA,uBAAA;AAAA,kBAAA,MAAA,EAAA;AAAA,iBAAA,EAAA,IAAA,CAAA,YAAA;AACA,kBAAA,MAAA,CAAA,MAAA,CAAA,QAAA,CAAA,YAAA;AACA,iBAFA,CADA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAIA,KAfA;AAgBA,IAAA,UAhBA,sBAgBA,GAhBA,EAgBA;AACA,aAAA,GAAA,CACA,KADA,CACA,EADA,EAEA,OAFA,GAGA,MAHA,CAGA,UAAA,IAAA,EAAA,IAAA,EAAA,KAAA,EAAA;AACA,eAAA,CAAA,KAAA,GAAA,CAAA,GAAA,IAAA,GAAA,IAAA,GAAA,GAAA,IAAA,IAAA;AACA,OALA,CAAA;AAMA;AAvBA;AAnCA,CAAA","sourcesContent":["<template>\n  <div class=\"d-flex justify-content-center flex-wrap\">\n    <div\n      v-for=\"item in this.$store.state.food.foods\"\n      :key=\"item\"\n      class=\"mt-3 ml-2\"\n    >\n      <div class=\"food\" @click=\"showModal(item.foodId)\">\n        <div class=\"d-flex justify-content-center\">\n          <h6>{{ item.foodName.slice(0, 20) }}</h6>\n        </div>\n        <div class=\"d-flex justify-content-center\">\n          <img class=\"img_list\" :src=\"item.images[0].imageUrl\" alt=\"\" />\n        </div>\n\n        <div class=\"d-flex justify-content-center align-items-center mt-2\">\n          <p class=\"mr-2 mt-2\">{{ formatCash(item.price) }}</p>\n          <b-button\n            variant=\"success\"\n            size=\"sm\"\n            class=\"mb-2\"\n            @click.stop=\"addToCard(item.foodId)\"\n            >Mua ngay</b-button\n          >\n        </div>\n      </div>\n    </div>\n    <b-pagination\n      class=\"mt-4\"\n      v-model=\"currentPage\"\n      :total-rows=\"rows\"\n      :per-page=\"perPage\"\n      aria-controls=\"my-table\"\n    ></b-pagination>\n    <b-modal ref=\"modal-1\" hide-footer hide-header-close hide-header>\n      <div>\n        <h4>{{ nameFood }}</h4>\n        <div class=\"d-flex justify-content-center\">\n          <img :src=\"image\" class=\"img_detail\" />\n        </div>\n        <p>{{ description }}</p>\n        <div class=\"d-flex justify-content-between\">\n          <b-button\n            variant=\"success\"\n            size=\"sm\"\n            class=\"mb-2\"\n            @click=\"addToCard(id_food)\"\n            >Mua ngay</b-button\n          >\n          <h5>{{ price }}Ä‘</h5>\n        </div>\n      </div>\n    </b-modal>\n  </div>\n</template>\n<script>\nimport { HTTP } from \"../API/http_common\";\nexport default {\n  data() {\n    return {\n      perPage: 4,\n      currentPage: 1,\n      nameFood: \"\",\n      imgage: \"\",\n      description: \"\",\n      price: \"\",\n      id_food: \"\",\n    };\n  },\n  created() {\n    this.$store.dispatch(\"getfood\", 0);\n  },\n  computed: {\n    rows() {\n      return this.$store.state.food.foods.length;\n    },\n  },\n  watch: {\n    currentPage: {\n      immediate: true,\n      handler(data) {\n        if (data !== 1) {\n          this.$store.dispatch(\n            \"getfood\",\n            this.$store.state.food.foods[11].updatedAt\n          );\n        } else {\n          this.$store.dispatch(\"getfood\", 0);\n        }\n      },\n    },\n  },\n  methods: {\n    async showModal(id_food) {\n      await HTTP.get(`/food/detail/${id_food}`).then((res) => {\n        this.nameFood = res.data.data.foodName;\n        this.image = res.data.data.images[0].imageUrl;\n        this.description = res.data.data.description;\n        this.price = res.data.data.price;\n        this.id_food = id_food;\n      });\n      this.$refs[\"modal-1\"].show();\n    },\n    async addToCard(foodId) {\n      await HTTP.post(`/order/add-to-cart`, { foodId: foodId }).then(() => {\n        this.$store.dispatch(\"getNumQuan\");\n      });\n    },\n    formatCash(str) {\n      return str\n        .split(\"\")\n        .reverse()\n        .reduce((prev, next, index) => {\n          return (index % 3 ? next : next + \",\") + prev;\n        });\n    },\n  },\n};\n</script>\n<style lang='scss' src='../style.scss' scoped/>"],"sourceRoot":"src/components"}]}