\"sm\" class=\"mb-2\">Mua ngay</b-button>\n          <h5>{{ price }}Ä‘</h5>\n        </div>\n      </div>\n    </b-modal>\n  </div>\n</template>\n<script>\nimport { HTTP } from \"../API/http_common\";\nexport default {\n  data() {\n    return {\n      food: [],\n      perPage: 4,\n      currentPage: 1,\n      nameFood: \"\",\n      imgage: \"\",\n      description: \"\",\n      price: \"\",\n    };\n  },\n  created() {\n    HTTP.get(\"/food/list/0/12\").then((res) => {\n      this.food = res.data.data;\n    });\n  },\n  computed: {\n    rows() {\n      return this.food.length;\n    },\n  },\n  watch: {\n    currentPage: {\n      immediate: true,\n      handler(data) {\n        if (data !== 1) {\n          HTTP.get(`/food/list/${this.food[11].updatedAt}/12`).then((res) => {\n            this.food = res.data.data;\n            console.log(this.food);\n          });\n        } else {\n          HTTP.get(\"/food/list/0/12\").then((res) => {\n            this.food = res.data.data;\n          });\n        }\n      },\n    },\n  },\n  methods: {\n    async showModal(id_food) {\n      await HTTP.get(`/food/detail/${id_food}`).then((res) => {\n        this.nameFood = res.data.data.foodName;\n        this.image = res.data.data.images[0].imageUrl;\n        this.description = res.data.data.description;\n        this.price = res.data.data.price;\n      });\n      this.$refs[\"modal-1\"].show();\n    },\n  },\n};\n</script>\n<style lang='scss' src='../style.scss' scoped/>"],"sourceRoot":"src/components"}]}