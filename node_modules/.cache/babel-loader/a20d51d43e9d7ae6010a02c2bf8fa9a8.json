AAA;;AAAA;AAAA,sBAUA,KAAA,CAAA,MAAA,CAAA,KAAA,CAAA,KAAA,CAAA,OAAA,KAAA,KAVA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAWA,IAAA,CAAA,GAAA,CAAA,YAAA,EAAA,KAAA,CAAA,WAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,sBAAA,GAAA,CAAA,MAAA,KAAA,GAAA,EAAA;AACA,oBAAA,KAAA,CAAA,KAAA,CAAA,MAAA;AACA,mBAFA,MAEA;AACA,oBAAA,KAAA,CAAA,qBAAA,CAAA;AACA;AACA,iBANA,CAXA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAmBA;AApBA;AArBA,CAAA","sourcesContent":["<template>\n  <b-container>\n    <b-form @submit=\"onSubmit\">\n      <div v-for=\"(user, i) in customer\" :key=\"i\">\n        <b-form-group :label=\"user.label\" :label-for=\"`input-${i}`\">\n          <v-radio-group\n            v-if=\"user.key === 'gioitinh'\"\n            :id=\"`input-${i}`\"\n            v-model=\"objCustomer[user.key]\"\n            required\n            row\n          >\n            <v-radio label=\"Male\" :value=\"0\"></v-radio>\n            <v-radio label=\"Female\" :value=\"1\"></v-radio>\n          </v-radio-group>\n          <b-form-input\n            v-else\n            :id=\"`input-${i}`\"\n            v-model=\"objCustomer[user.key]\"\n            :type=\"user.type\"\n            :placeholder=\"`Nhập ${user.label}`\"\n            required\n          ></b-form-input>\n        </b-form-group>\n      </div>\n      <div class=\"d-flex justify-content-end\">\n        <b-button type=\"submit\" variant=\"primary\" class=\"mt-3\">Submit</b-button>\n      </div>\n    </b-form>\n  </b-container>\n</template>\n<script>\nimport { objCustomer, customer } from \"./constant\";\nimport { HTTP } from \"../../API/http_common\";\nexport default {\n  data() {\n    return {\n      objCustomer,\n      customer,\n    };\n  },\n  props: {\n    items: {\n      type: Object,\n      default: () => {},\n    },\n  },\n  created() {\n    if (this.$store.state.users.addEdit === false) {\n      this.objCustomer = this.items;\n      this.objCustomer.username = this.items.taikhoan.username;\n      this.objCustomer.password = this.items.taikhoan.password;\n      delete this.objCustomer.khachhang;\n    }\n  },\n  methods: {\n    async onSubmit(event) {\n      event.preventDefault();\n      if (this.$store.state.users.addEdit === true) {\n        await HTTP.post(\"/register\", this.objCustomer).then((res) => {\n          if (res.status === 200) {\n            this.$emit(\"done\");\n          } else {\n            alert(\"Thêm khách thất bại\");\n          }\n        });\n      } else if (this.$store.state.users.addEdit === false) {\n        await HTTP.put(\"/khachhang\", this.objCustomer).then((res) => {\n          if (res.status === 200) {\n            this.$emit(\"done\");\n          } else {\n            alert(\"Thêm khách thất bại\");\n          }\n        });\n      }\n    },\n  },\n};\n</script>"],"sourceRoot":"src/components/AdminPage"}]}