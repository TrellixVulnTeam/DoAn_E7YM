AA;AACA,KAxEA;AAyEA,IAAA,MAzEA,oBAyEA;AACA,UAAA,OAAA,CAAA,gCAAA,CAAA,EAAA;AACA,QAAA,YAAA,CAAA,UAAA,CAAA,OAAA;AACA,aAAA,OAAA,CAAA,IAAA,CAAA,GAAA;AACA;AACA,KA9EA;AA+EA,IAAA,MA/EA,oBA+EA;AAAA;;AACA,MAAA,IAAA,CAAA,GAAA,CAAA,YAAA,EAAA;AACA,QAAA,EAAA,EAAA,KAAA,IAAA,CAAA,EADA;AAEA,QAAA,GAAA,EAAA,KAAA,IAAA,CAAA,GAFA;AAGA,QAAA,KAAA,EAAA,KAAA,IAAA,CAAA,KAHA;AAIA,QAAA,GAAA,EAAA,KAAA,IAAA,CAAA,GAJA;AAKA,QAAA,MAAA,EAAA,KAAA,IAAA,CAAA,MALA;AAMA,QAAA,QAAA,EAAA,KAAA,IAAA,CAAA,QANA;AAOA,QAAA,QAAA,EAAA,KAAA,IAAA,CAAA,QAAA,CAAA;AAPA,OAAA,EAQA,IARA,CAQA,YAAA;AACA,QAAA,MAAA,CAAA,IAAA,GAAA,KAAA;AACA,OAVA;AAWA,KA3FA;AA4FA,IAAA,QA5FA,sBA4FA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,MAAA,CAAA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,MAAA,KAAA,CADA;AAAA;AAAA;AAAA;;AAAA,kDAEA,KAAA,CAAA,gCAAA,CAFA;;AAAA;AAAA;AAAA,uBAIA,IAAA,CAAA,IAAA,oBACA,MAAA,CAAA,MAAA,CAAA,KAAA,CAAA,KAAA,CAAA,KAAA,CAAA,IADA,GAEA,MAAA,CAAA,KAFA,EAGA,IAHA,CAGA,YAAA;AACA,kBAAA,MAAA,CAAA,QAAA;;AACA,kBAAA,MAAA,CAAA,MAAA,CAAA,QAAA,CAAA,YAAA,EAAA,MAAA,CAAA,MAAA,CAAA,KAAA,CAAA,KAAA,CAAA,KAAA,CAAA,IAAA;;AACA,kBAAA,MAAA,CAAA,MAAA,CAAA,QAAA,CAAA,WAAA,EAAA,MAAA,CAAA,MAAA,CAAA,KAAA,CAAA,KAAA,CAAA,KAAA,CAAA,IAAA;AACA,iBAPA,CAJA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAYA,KAxGA;AAyGA,IAAA,YAzGA,0BAyGA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,sBACA,MAAA,CAAA,MAAA,CAAA,KAAA,CAAA,IAAA,CAAA,MAAA,KAAA,CADA;AAAA;AAAA;AAAA;;AAAA,kDAEA,KAAA,CAAA,6BAAA,CAFA;;AAAA;AAAA;AAAA,uBAIA,IAAA,CAAA,MAAA,oBAAA,MAAA,CAAA,MAAA,CAAA,KAAA,CAAA,KAAA,CAAA,KAAA,CAAA,IAAA,GAAA,IAAA,CACA,YAAA;AACA,kBAAA,MAAA,CAAA,QAAA;;AACA,kBAAA,MAAA,CAAA,MAAA,CAAA,QAAA,CACA,YADA,EAEA,MAAA,CAAA,MAAA,CAAA,KAAA,CAAA,KAAA,CAAA,KAAA,CAAA,IAFA;AAIA,iBAPA,CAJA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAaA,KAtHA;AAuHA,IAAA,QAvHA,sBAuHA;AACA,WAAA,IAAA,GAAA,IAAA;AACA;AAzHA,GADA;AA6HA,EAAA,IA7HA,kBA6HA;AACA,WAAA;AACA,MAAA,OAAA,EAAA,CADA;AAEA,MAAA,KAAA,EAAA,EAFA;AAGA,MAAA,IAAA,EAAA,EAHA;AAIA,MAAA,IAAA,EAAA,KAJA;AAKA,MAAA,SAAA,EAAA,EALA;AAMA,MAAA,KAAA,EAAA,CANA;AAOA,MAAA,KAAA,EAAA,EAPA;AAQA,MAAA,KAAA,EAAA,CAAA,cAAA,EAAA,aAAA,EAAA,cAAA,EAAA,eAAA;AARA,KAAA;AAUA,GAxIA;AAyIA,EAAA,QAAA,EAAA;AACA,IAAA,OADA,qBACA;AACA,aAAA,YAAA,CAAA,OAAA,CAAA,OAAA,CAAA;AACA;AAHA,GAzIA;AA8IA,EAAA,OA9IA,qBA8IA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,mBACA,YAAA,CAAA,OAAA,CAAA,OAAA,CADA;AAAA;AAAA;AAAA;;AAEA,kBAAA,IAAA,CAAA,KAAA,CAAA,YAAA,CAAA,OAAA,CAAA,OAAA,CAAA,EAAA,KAAA,CAAA,OAAA,KAAA,CAAA,EAAA;AACA,gBAAA,MAAA,CAAA,OAAA,CAAA,IAAA,CAAA,YAAA;AACA;;AACA,cAAA,KALA,GAKA,YAAA,CAAA,OAAA,CAAA,OAAA,CALA;AAAA;AAAA,qBAMA,IAAA,CAAA,GAAA,sBAAA,IAAA,CAAA,KAAA,CAAA,KAAA,EAAA,QAAA,GAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,gBAAA,MAAA,CAAA,IAAA,GAAA,GAAA,CAAA,IAAA;AACA,gBAAA,MAAA,CAAA,IAAA,CAAA,QAAA,GAAA,IAAA,CAAA,KAAA,CAAA,KAAA,EAAA,KAAA,CAAA,QAAA;;AACA,gBAAA,MAAA,CAAA,MAAA,CAAA,QAAA,CAAA,WAAA,EAAA,GAAA,CAAA,IAAA;AACA,eAJA,CANA;;AAAA;AAWA,cAAA,MAAA,CAAA,MAAA,CAAA,QAAA,CAAA,YAAA,EAAA,MAAA,CAAA,MAAA,CAAA,KAAA,CAAA,KAAA,CAAA,KAAA,CAAA,IAAA;;AACA,cAAA,MAAA,CAAA,MAAA,CAAA,QAAA,CAAA,WAAA,EAAA,MAAA,CAAA,MAAA,CAAA,KAAA,CAAA,KAAA,CAAA,KAAA,CAAA,IAAA;;AAZA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAcA;AA5JA,CAAA","sourcesContent":["<template>\n  <v-app>\n    <v-app-bar app dark color=\"rgb(116, 116, 252)\">\n      <v-toolbar-title @click=\"homepage()\" style=\"cursor: pointer\">\n        <img height=\"50px\" src=\"../assets/app.png\" />\n        App Cosmetic</v-toolbar-title\n      >\n      <v-spacer />\n      <v-btn text rounded v-if=\"isLogin\" v-b-toggle.sidebar-right\n        ><img\n          class=\"icon\"\n          src=\"https://cdn.pixabay.com/photo/2015/12/23/01/14/edit-1105049_960_720.png\"\n        />\n        <div class=\"number d-flex justify-content-center align-items-start\">\n          <p class=\"num mt-1\">{{ this.$store.state.food.Quanty }}</p>\n        </div></v-btn\n      >\n      <v-btn v-if=\"isLogin\" text rounded v-b-toggle.sidebar-variant\n        ><img\n          class=\"icon\"\n          src=\"https://freeiconshop.com/wp-content/uploads/edd/clipboard-list-flat.png\"\n        />\n        <div class=\"number d-flex justify-content-center align-items-start\">\n          <p class=\"num mt-1\">{{ this.$store.state.food.bill.length }}</p>\n        </div></v-btn\n      >\n      <v-btn v-if=\"isLogin\" text rounded v-b-toggle.sidebar-1\n        ><img\n          class=\"icon user\"\n          src=\"https://www.ebanhang.vn/app/webroot/img/icon_login.png\"\n      /></v-btn>\n      <b-breadcrumb class=\"mt-4\" v-if=\"!isLogin\">\n        <b-breadcrumb-item to=\"/\">\n          <b-button variant=\"outline-light\"\n            ><b-icon class=\"mr-1\" icon=\"reply-all-fill\" aria-hidden=\"true\"></b-icon>Đăng\n            nhập</b-button\n          >\n        </b-breadcrumb-item>\n        <b-breadcrumb-item to=\"register\"\n          ><b-button variant=\"dark\"\n            ><b-icon class=\"mr-1\" icon=\"shield-fill-plus\" aria-hidden=\"true\"></b-icon>Đăng\n            ký</b-button\n          ></b-breadcrumb-item\n        >\n      </b-breadcrumb>\n    </v-app-bar>\n    <v-content>\n      <router-view />\n    </v-content>\n    <v-footer dark padless>\n      <v-card flat tile class=\"indigo lighten-1 white--text text-center\">\n        <v-card-text>\n          <v-btn\n            v-for=\"icon in icons\"\n            :key=\"icon\"\n            class=\"mx-4 white--text\"\n            icon\n          >\n            <v-icon size=\"24px\">\n              {{ icon }}\n            </v-icon>\n          </v-btn>\n        </v-card-text>\n\n        <v-card-text class=\"white--text pt-0\">\n          Phasellus feugiat arcu sapien, et iaculis ipsum elementum sit amet.\n          Mauris cursus commodo interdum. Praesent ut risus eget metus luctus\n          accumsan id ultrices nunc. Sed at orci sed massa consectetur dignissim\n          a sit amet dui. Duis commodo vitae velit et faucibus. Morbi vehicula\n          lacinia malesuada. Nulla placerat augue vel ipsum ultrices, cursus\n          iaculis dui sollicitudin. Vestibulum eu ipsum vel diam elementum\n          tempor vel ut orci. Orci varius natoque penatibus et magnis dis\n          parturient montes, nascetur ridiculus mus.\n        </v-card-text>\n\n        <v-divider></v-divider>\n\n        <v-card-text class=\"white--text\">\n          {{ new Date().getFullYear() }} — <strong>Vuetify</strong>\n        </v-card-text>\n      </v-card>\n    </v-footer>\n    <b-sidebar\n      @shown=\"showCart\"\n      id=\"sidebar-right\"\n      title=\"Giỏ Hàng\"\n      bg-variant=\"dark\"\n      text-variant=\"light\"\n      right\n      backdrop\n      width=\"550px\"\n      shadow\n    >\n      <template #footer=\"{}\">\n        <div class=\"d-flex bg-dark text-light align-items-center px-3 py-2\">\n          <strong class=\"mr-auto\">Tổng</strong>\n          <strong class=\"mr-auto\">{{ items ? formatCash(total) : 0 }}</strong>\n          <b-button size=\"sm\" class=\"mr-2\" variant=\"danger\" v-b-modal.modal-2\n            >Xóa giỏ hàng</b-button\n          >\n          <b-button size=\"sm\" variant=\"info\" v-b-modal.modal-1\n            >Thanh Toán</b-button\n          >\n        </div>\n      </template>\n      <div class=\"px-3 py-2\">\n        <b-row>\n          <b-col><b>Tên món</b></b-col>\n          <b-col><b>Hình ảnh</b></b-col>\n          <b-col><b>Số lượng</b></b-col>\n          <b-col><b>Đơn giá</b></b-col>\n          <b-col></b-col>\n        </b-row>\n        <div v-for=\"item in items\" :key=\"item\">\n          <b-row>\n            <b-col>{{ item.sanpham.tensp }}</b-col>\n            <b-col\n              ><img class=\"img_cart\" :src=\"item.sanpham.photo\" alt=\"\"\n            /></b-col>\n            <b-col\n              ><img\n                class=\"mr-2\"\n                @click=\"editQuanty(item, '-1')\"\n                height=\"10px\"\n                style=\"cursor: pointer\"\n                src=\"../assets/minus.png\" /><input\n                type=\"number\"\n                min=\"0\"\n                class=\"input\"\n                v-model=\"item.soluong\"\n                disabled />\n              <img\n                @click=\"editQuanty(item, '1')\"\n                style=\"cursor: pointer\"\n                class=\"ml-2\"\n                height=\"10px\"\n                src=\"../assets/plus.png\"\n            /></b-col>\n            <b-col>{{ formatCash(+item.sanpham.dongia) }}</b-col>\n            <b-col\n              ><img\n                @click=\"deleteProduct(item.sanpham.masp)\"\n                class=\"icon_delete\"\n                src=\"https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTNQ8CLeXOGmVx62VfeFd3eH2elD1EUYLJ0WISuby6szkUl3P6PLW6Djxuy1yOlLkcCMNs&usqp=CAU\"\n            /></b-col>\n          </b-row>\n        </div>\n      </div>\n    </b-sidebar>\n    <b-sidebar id=\"sidebar-1\" title=\"Thông tin cá nhân\" shadow backdrop>\n      <template #footer=\"{}\">\n        <div\n          class=\"d-flex justify-content-between align-items-center px-3 py-2\"\n        >\n          <b-button size=\"sm\" variant=\"warning\" @click=\"editInfo\"\n            >Sửa thông tin</b-button\n          >\n          <b-button size=\"sm\" variant=\"danger\" @click=\"Logout\"\n            >Đăng xuất</b-button\n          >\n        </div>\n      </template>\n      <div class=\"px-3 py-2\">\n        <div v-if=\"!edit\">\n          <p>Họ tên : {{ user.ho + \" \" + user.ten }}</p>\n          <p>Email : {{ user.email }}</p>\n          <p>Số điện thoại : {{ user.sdt }}</p>\n          <p>Địa chỉ : {{ user.diachi }}</p>\n          <p>Phân Quyền : {{ user.tenquyen }}</p>\n        </div>\n\n        <div v-if=\"edit\">\n          <div class=\"d-flex justify-content-center\">\n            <p>Họ</p>\n            <b-form-input class=\"input\" v-model=\"user.ho\"></b-form-input>\n            <p>Tên</p>\n            <b-form-input class=\"input\" v-model=\"user.ten\"></b-form-input>\n          </div>\n\n          <div class=\"d-flex justify-content-center\">\n            <p>Địa chỉ</p>\n            <b-form-input class=\"input\" v-model=\"user.diachi\"></b-form-input>\n          </div>\n          <div class=\"d-flex justify-content-center\">\n            <p>Số điện thoại</p>\n            <b-form-input class=\"input\" v-model=\"user.sdt\"></b-form-input>\n          </div>\n          <div class=\"d-flex justify-content-center\">\n            <p>Email</p>\n            <b-form-input class=\"input\" v-model=\"user.email\"></b-form-input>\n          </div>\n          <v-radio-group v-model=\"user.gioitinh\" row>\n            <template v-slot:label>\n              <div>Gender :</div>\n            </template>\n            <v-radio label=\"Male\" :value=\"0\"></v-radio>\n            <v-radio label=\"Female\" :value=\"1\"></v-radio>\n          </v-radio-group>\n          <b-button size=\"sm\" variant=\"success\" @click=\"submit\"\n            >Submit</b-button\n          >\n        </div>\n        <b-img\n          src=\"https://picsum.photos/500/500/?image=54\"\n          fluid\n          thumbnail\n        ></b-img>\n      </div>\n    </b-sidebar>\n    <b-modal id=\"modal-1\" title=\"Xác nhận thanh toán\" @ok=\"handleOk\">\n      <p class=\"my-4\">\n        Bạn sẽ thanh toán hóa đơn với số tiền\n        {{ items ? formatCash(total) : \"\" }} ?\n      </p>\n    </b-modal>\n    <b-modal id=\"modal-2\" title=\"Xác nhận thanh toán\" @ok=\"handleDelete\">\n      <p class=\"my-4\">Bạn có chắc chắn xóa giỏ hàng ?</p>\n    </b-modal>\n    <b-sidebar\n      id=\"sidebar-variant\"\n      title=\"Danh sách đơn hàng\"\n      bg-variant=\"dark\"\n      width=\"450px\"\n      backdrop\n      text-variant=\"light\"\n      shadow\n    >\n      <div class=\"px-3 py-2\">\n        <b-row>\n          <b-col cols=\"3\"><b>Mã Đơn</b></b-col>\n          <b-col><b>Trạng Thái</b></b-col>\n          <b-col><b>Ngày Đặt</b></b-col>\n          <b-col><b>Hành động</b></b-col>\n        </b-row>\n        <div v-for=\"item in this.$store.state.food.bill\" :key=\"item\">\n          <b-row @click=\"detail(item.madh)\">\n            <b-col cols=\"3\">{{ item.madh }}</b-col>\n            <b-col\n              ><b-button :variant=\"handleStatus(item.trangthai).color\">{{\n                handleStatus(item.trangthai).text\n              }}</b-button></b-col\n            >\n            <b-col>{{ item.ngaydat | moment(\"DD/MM/YYYY\") }}</b-col>\n            <b-col\n              ><b-button\n                :disabled=\"item.trangthai !== 0\"\n                @click.stop=\"cancelBill(item.madh)\"\n                variant=\"light\"\n                >Hủy đơn</b-button\n              ></b-col\n            >\n          </b-row>\n        </div>\n      </div>\n      <div class=\"px-3 py-2\">\n        <b-table striped hover :items=\"orderList\"></b-table>\n      </div>\n    </b-sidebar>\n  </v-app>\n</template>\n\n<script>\nimport { HTTP } from \"../API/http_common\";\nexport default {\n  methods: {\n    handleStatus(status) {\n      let temp = this.$store.state.food.status.find((e) => {\n        return e.value === status;\n      });\n      return temp;\n    },\n    async cancelBill(madh) {\n      if (confirm(\"Bạn có chắc chắn hủy đơn\")) {\n        try {\n          await HTTP.put(`/donhang/${madh}`);\n          await this.$store.dispatch(\n            \"listBills\",\n            this.$store.state.users.users.makh\n          );\n        } catch (error) {\n          alert(\"Hủy đơn hàng thất bại\");\n        }\n      }\n    },\n    formatCash(value) {\n      let val = (value / 1).toFixed(0).replace(\".\", \",\");\n      return val.toString().replace(/\\B(?=(\\d{3})+(?!\\d))/g, \",\");\n    },\n    async showCart() {\n      await HTTP.get(`/giohang/${this.$store.state.users.users.makh}`).then(\n        (res) => {\n          this.items = res.data;\n          if (this.items.length === 0) {\n            this.total = 0;\n          }\n          this.items.forEach((e) => {\n            if (e.soluong === 0) {\n              this.deleteProduct(e.id.masp);\n            }\n          });\n          this.order = this.items.map((e) => {\n            return {\n              masp: e.id.masp,\n              soluong: e.soluong,\n              dongia: e.sanpham.dongia,\n            };\n          });\n          let temp = this.items.map((e) => {\n            return e.sanpham.dongia * e.soluong;\n          });\n          this.total = temp.reduce((total, currentValue) => {\n            return total + currentValue;\n          });\n        }\n      );\n    },\n    editQuanty(item, isPlus) {\n      HTTP.post(\n        `/giohang/${this.$store.state.users.users.makh}/${item.id.masp}?soluong=${isPlus}`\n      ).then(() => {\n        this.showCart();\n        this.$store.dispatch(\"getNumQuan\", this.$store.state.users.users.makh);\n      });\n    },\n    async deleteProduct(foodId) {\n      await this.$store.dispatch(\"deleteBills\", {\n        foodId,\n        makh: this.$store.state.users.users.makh,\n      });\n      setTimeout(() => {\n        this.showCart();\n        this.$store.dispatch(\"getNumQuan\", this.$store.state.users.users.makh);\n      }, 100);\n    },\n    detail(orderId) {\n      this.$router.push(`detail/${orderId}`);\n    },\n    Logout() {\n      if (confirm(\"Bạn chắc chắn muốn đăng xuất ?\")) {\n        localStorage.removeItem(\"token\");\n        this.$router.push(\"/\");\n      }\n    },\n    submit() {\n      HTTP.put(\"/khachhang\", {\n        ho: this.user.ho,\n        ten: this.user.ten,\n        email: this.user.email,\n        sdt: this.user.sdt,\n        diachi: this.user.diachi,\n        gioitinh: this.user.gioitinh,\n        username: this.user.taikhoan.username,\n      }).then(() => {\n        this.edit = false;\n      });\n    },\n    async handleOk() {\n      if (this.$store.state.food.Quanty === 0) {\n        return alert(\"Vui lòng chọn ít nhất 1 món ăn\");\n      }\n      await HTTP.post(\n        `/donhang/${this.$store.state.users.users.makh}`,\n        this.order\n      ).then(() => {\n        this.showCart();\n        this.$store.dispatch(\"getNumQuan\", this.$store.state.users.users.makh);\n        this.$store.dispatch(\"listBills\", this.$store.state.users.users.makh);\n      });\n    },\n    async handleDelete() {\n      if (this.$store.state.food.Quanty === 0) {\n        return alert(\"Giỏ hàng không còn sản phẩm\");\n      }\n      await HTTP.delete(`/giohang/${this.$store.state.users.users.makh}`).then(\n        () => {\n          this.showCart();\n          this.$store.dispatch(\n            \"getNumQuan\",\n            this.$store.state.users.users.makh\n          );\n        }\n      );\n    },\n    editInfo() {\n      this.edit = true;\n    },\n  },\n\n  data() {\n    return {\n      numQuan: 0,\n      items: [],\n      user: {},\n      edit: false,\n      orderList: [],\n      total: 0,\n      order: [],\n      icons: [\"mdi-facebook\", \"mdi-twitter\", \"mdi-linkedin\", \"mdi-instagram\"],\n    };\n  },\n  computed: {\n    isLogin() {\n      return localStorage.getItem(\"token\");\n    },\n  },\n  async created() {\n    if (localStorage.getItem(\"token\")) {\n      if (JSON.parse(localStorage.getItem(\"token\")).quyen.maquyen !== 2) {\n        this.$router.push(\"/not-found\");\n      }\n      let token = localStorage.getItem(\"token\");\n      await HTTP.get(`/khachhang/${JSON.parse(token).username}`).then((res) => {\n        this.user = res.data;\n        this.user.tenquyen = JSON.parse(token).quyen.tenquyen;\n        this.$store.dispatch(\"loadUsers\", res.data);\n      });\n      this.$store.dispatch(\"getNumQuan\", this.$store.state.users.users.makh);\n      this.$store.dispatch(\"listBills\", this.$store.state.users.users.makh);\n    }\n  },\n};\n</script>\n<style lang='scss' src='../style.scss' >\n</style> \n"],"sourceRoot":"src/components"}]}